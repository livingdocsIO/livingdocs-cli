{
  "name": "living-stories",
  "label": "Living Stories",
  "version": "0.0.2",
  "license": "Copyright (c) 2018 Livingdocs AG, all rights reserved",
  "assets": {
    "basePath": "https://cdn-jbxbgjz.livingdocs.io/designs/living-stories/0.0.2",
    "css": [
      "./styles.css"
    ],
    "js": [
      "./scripts.js"
    ]
  },
  "componentProperties": {
    "paragraph-style-lead": {
      "label": "Lead",
      "type": "option",
      "value": "paragraph--lead"
    },
    "position": {
      "label": "Position",
      "type": "select",
      "options": [
        {
          "caption": "Middle"
        },
        {
          "caption": "Left",
          "value": "left"
        },
        {
          "caption": "Right",
          "value": "right"
        }
      ]
    },
    "image-position": {
      "label": "Position",
      "type": "select",
      "options": [
        {
          "caption": "Middle"
        },
        {
          "caption": "Left",
          "value": "image--left"
        },
        {
          "caption": "Right",
          "value": "image--right"
        }
      ]
    },
    "teaser-feature-style": {
      "label": "Style",
      "type": "select",
      "options": [
        {
          "caption": "Big"
        },
        {
          "caption": "Small",
          "value": "teaser-feature-card--small"
        }
      ]
    },
    "image-width": {
      "label": "Image width",
      "type": "select",
      "options": [
        {
          "caption": "Normal"
        },
        {
          "caption": "Wide",
          "value": "image--wide"
        }
      ]
    },
    "container-style-breath": {
      "label": "Extra margin",
      "type": "option",
      "value": "container--breath"
    },
    "container-style-lined": {
      "label": "Add lines",
      "type": "option",
      "value": "container--lined"
    },
    "options": {
      "id": "Options",
      "label": "Options",
      "type": "select",
      "options": [
        {
          "caption": "Default"
        },
        {
          "caption": "Stripe",
          "value": "half-height"
        }
      ]
    },
    "featured": {
      "label": "Featured",
      "type": "option",
      "value": "bleed"
    },
    "unordered": {
      "label": "Unordered",
      "type": "option",
      "value": "bullet"
    },
    "large": {
      "label": "Large",
      "type": "option",
      "value": "large"
    },
    "list-type": {
      "label": "List Type",
      "type": "select",
      "options": [
        {
          "caption": "Bullets"
        },
        {
          "caption": "Numbers",
          "value": "decimal"
        },
        {
          "caption": "Letters",
          "value": "lower-latin"
        },
        {
          "caption": "Romans",
          "value": "lower-roman"
        }
      ]
    }
  },
  "layouts": [
    {
      "name": "regular",
      "caption": "Article",
      "wrapper": "<div class=\"doc-section wrapper wrapper--article\"></div>",
      "defaultContent": [
        {
          "identifier": "living-stories.article-container",
          "containers": {
            "header": [
              {
                "identifier": "living-stories.head"
              }
            ],
            "main": [
              {
                "identifier": "living-stories.image"
              },
              {
                "identifier": "living-stories.paragraph"
              },
              {
                "identifier": "living-stories.teaser-author"
              }
            ]
          }
        }
      ],
      "groups": [
        {
          "label": "Headers",
          "components": [
            "head",
            "head-wide",
            "subtitle"
          ]
        },
        {
          "label": "Text",
          "components": [
            "paragraph",
            "quote",
            "separator",
            "bullet-list",
            "event-list"
          ]
        },
        {
          "label": "Images",
          "components": [
            "image",
            "image-row",
            "image-full-bleed"
          ]
        },
        {
          "label": "Embeds",
          "components": [
            "tweet",
            "iframe",
            "free-html"
          ]
        },
        {
          "label": "Teasers",
          "components": [
            "teaser-author",
            "teaser-card",
            "teaser-gallery-hero"
          ]
        }
      ]
    },
    {
      "name": "gallery",
      "caption": "Gallery",
      "wrapper": "<div class=\"doc-section wrapper wrapper--article\"></div>",
      "defaultContent": [
        {
          "identifier": "living-stories.article-container",
          "containers": {
            "header": [
              {
                "identifier": "living-stories.head"
              }
            ],
            "main": [
              {
                "identifier": "living-stories.gallery-container",
                "containers": {
                  "gallery": [
                    {
                      "identifier": "living-stories.image"
                    },
                    {
                      "identifier": "living-stories.image"
                    }
                  ]
                }
              }
            ]
          }
        }
      ],
      "groups": [
        {
          "label": "Headers",
          "components": [
            "head"
          ]
        },
        {
          "label": "Image",
          "components": [
            "image"
          ]
        }
      ]
    },
    {
      "wrapper": "<div class=\"doc-section wrapper wrapper--default\"></div>",
      "name": "author",
      "caption": "Author",
      "defaultContent": []
    }
  ],
  "defaultLayout": "regular",
  "defaultComponents": {
    "paragraph": "paragraph",
    "image": "image",
    "html": "free-html"
  },
  "imageRatios": {
    "16:9": {
      "label": "16:9",
      "ratio": "16x9"
    },
    "1:1": {
      "label": "1:1",
      "ratio": "1x1"
    },
    "4:3": {
      "label": "4:3",
      "ratio": "4x3"
    },
    "3:4": {
      "label": "3:4",
      "ratio": "3x4"
    }
  },
  "prefilledComponents": {
    "head": {
      "author": {
        "value": "author",
        "type": "computed"
      }
    }
  },
  "metadata": [
    {
      "identifier": "title",
      "type": "text",
      "matches": [
        "head.title"
      ],
      "isEditable": true
    },
    {
      "identifier": "description",
      "type": "text",
      "matches": [
        "head.text",
        "paragraph.text"
      ],
      "isEditable": true
    },
    {
      "identifier": "teaserImage",
      "type": "image",
      "matches": [
        "image.image"
      ],
      "isEditable": true,
      "imageRatios": [
        "16:9"
      ]
    },
    {
      "identifier": "authorImage",
      "type": "image",
      "matches": [],
      "isEditable": true,
      "imageRatios": [
        "1:1"
      ]
    }
  ],
  "components": [
    {
      "name": "ad-rectangle-resolved",
      "html": "<div class=\"ad ad--sidebar\"><a href=\"https://www.livingdocs.io\" target=\"_blank\"><img style=\"max-height: 250px\" src=\"/assets/images/ads/rectangles/ad-standard.gif\" alt></a></div>",
      "label": "Ad half page"
    },
    {
      "name": "ad-leaderboard-resolved",
      "html": "<div class=\"ad ad--leaderboard\"><a href=\"https://www.livingdocs.io\" target=\"_blank\"><img style=\"max-height: 90px\" src=\"/assets/images/ads/leaderboards/ad-exclusive.gif\" alt></a></div>",
      "label": "Ad leaderboard"
    },
    {
      "name": "author-container",
      "html": "<article class=\"article-content\"><div class=\"article-content__body\"><main class=\"article-content__main\" doc-container=\"main\"></main></div></article>",
      "label": "Author Container",
      "description": "Main container for authors",
      "iconUrl": "https://livingdocs-assets.s3.amazonaws.com/magazine-design/assets/images/icons-components/icon_2_columns_uneven.svg"
    },
    {
      "name": "article-container",
      "html": "<article class=\"article-content\"><header class=\"article-content__header\" doc-container=\"header\"></header><div class=\"article-content__body\"><main class=\"article-content__main\" doc-container=\"main\"></main></div></article>",
      "label": "Article Container",
      "description": "Main container for articles",
      "iconUrl": "https://livingdocs-assets.s3.amazonaws.com/magazine-design/assets/images/icons-components/icon_2_columns_uneven.svg",
      "directives": {
        "header": {
          "allowedChildren": [
            "head",
            "head-wide"
          ],
          "defaultComponents": {
            "paragraph": "paragraph",
            "image": "image",
            "html": "free-html"
          }
        },
        "main": {
          "allowedChildren": [
            "free-html",
            "iframe",
            "tweet",
            "subtitle",
            "image",
            "image-row",
            "image-full-bleed",
            "bullet-list",
            "event-list",
            "paragraph",
            "separator",
            "quote",
            "whole",
            "teaser-author",
            "gallery-container",
            "teaser-gallery-hero",
            "teaser-gallery",
            "teaser-card"
          ],
          "defaultComponents": {
            "paragraph": "paragraph",
            "image": "image",
            "html": "free-html"
          }
        }
      }
    },
    {
      "name": "featured-container",
      "html": "<div class=\"feature-container\"><div class=\"feature-container__col\" doc-container=\"column-one\"></div><div class=\"feature-container__col\" doc-container=\"column-two\"></div></div>",
      "label": "Featured articles",
      "description": "Container for placing featured articles",
      "iconUrl": "https://livingdocs-assets.s3.amazonaws.com/magazine-design/assets/images/icons-components/icon_2_columns.svg",
      "directives": {
        "column-one": {
          "allowedChildren": [
            "teaser-card-featured",
            "teaser-card-featured-resolved",
            "teaser-card-featured-small",
            "teaser-card-featured-small-resolved"
          ],
          "defaultComponents": {
            "paragraph": "paragraph",
            "image": "image",
            "html": "free-html"
          }
        },
        "column-two": {
          "allowedChildren": [
            "teaser-card-featured",
            "teaser-card-featured-resolved",
            "teaser-card-featured-small",
            "teaser-card-featured-small-resolved"
          ],
          "defaultComponents": {
            "paragraph": "paragraph",
            "image": "image",
            "html": "free-html"
          }
        }
      }
    },
    {
      "name": "gallery-container",
      "html": "<div class=\"gallery-container\" doc-container=\"gallery\"></div>",
      "label": "Gallery Container",
      "description": "1 column to add images",
      "iconUrl": "https://livingdocs-assets.s3.amazonaws.com/magazine-design/assets/images/icons-components/icon_1_column.svg",
      "directives": {
        "gallery": {
          "allowedChildren": [
            "image"
          ],
          "defaultComponents": {
            "paragraph": "paragraph",
            "image": "image",
            "html": "free-html"
          }
        }
      }
    },
    {
      "name": "whole",
      "html": "<div class=\"container\"><h2 class=\"container__title\" doc-editable=\"title\" doc-optional=\"true\">Section title</h2><div class=\"container-grid container-grid--whole\"><div class=\"container-grid__item\" doc-container=\"column-one\"></div></div></div>",
      "label": "1 Column",
      "description": "One large column for content",
      "iconUrl": "https://livingdocs-assets.s3.amazonaws.com/magazine-design/assets/images/icons-components/icon_1_column.svg",
      "properties": [
        "container-style-breath",
        "container-style-lined"
      ],
      "directives": {
        "column-one": {
          "allowedChildren": [
            "free-html",
            "iframe",
            "tweet",
            "section-title",
            "subtitle",
            "image",
            "bullet-list",
            "paragraph",
            "separator",
            "quote",
            "teaser-card",
            "teaser-gallery-hero",
            "teaser-gallery",
            "ad-leaderboard-resolved"
          ],
          "defaultComponents": {
            "paragraph": "paragraph",
            "image": "image",
            "html": "free-html"
          }
        }
      }
    },
    {
      "name": "free-html",
      "html": "<div doc-html=\"free-html\" class=\"placeholder--free-html\"><div class=\"placeholder__img\"></div></div>",
      "label": "Free HTML",
      "description": "Only for developers",
      "allowedParents": [
        "article-container",
        "author-container",
        "gallery-container",
        "whole"
      ],
      "iconUrl": "https://livingdocs-assets.s3.amazonaws.com/magazine-design/assets/images/icons-components/icon_embed.svg"
    },
    {
      "name": "tweet",
      "html": "<div doc-html=\"tweet\" class=\"placeholder--twitter\"><div class=\"placeholder__img\"></div></div>",
      "label": "Tweet",
      "description": "Embed a tweet",
      "allowedParents": [
        "article-container",
        "author-container",
        "gallery-container",
        "whole"
      ],
      "iconUrl": "https://livingdocs-assets.s3.amazonaws.com/magazine-design/assets/images/icons-components/icon_twitter.svg"
    },
    {
      "name": "iframe",
      "html": "<div doc-html=\"iframe\" class=\"placeholder--free-html\"><div class=\"placeholder__img\"></div></div>",
      "label": "Iframe",
      "allowedParents": [
        "whole",
        "author-container",
        "article-container",
        "gallery-container"
      ],
      "description": "Embed anything an iframe can do",
      "iconUrl": "https://livingdocs-assets.s3.amazonaws.com/magazine-design/assets/images/icons-components/icon_embed.svg",
      "properties": [
        "position"
      ]
    },
    {
      "name": "event-list-item",
      "html": "<li class=\"event-list__item\"><div class=\"event-list__time\"><time doc-editable=\"time\">When? (e.g.: 28.11.18)</time> <span doc-editable=\"keyword\" doc-optional=\"true\">Keyword</span></div><p class=\"event-list__text\" doc-editable=\"text\">What happened?</p></li>",
      "label": "Event Item",
      "allowedParents": [
        "event-list"
      ]
    },
    {
      "name": "author-head",
      "html": "<div class=\"author-card author-card--big\"><div class=\"author-card__image\"><img doc-image=\"image\"></div><div class=\"author-card__body\"><h2 class=\"author-card__title\" doc-editable=\"name\">Author name</h2><p doc-editable=\"biography\">Biography</p></div></div>",
      "label": "Author Head",
      "description": "Header for author pages",
      "iconUrl": "https://livingdocs-assets.s3.amazonaws.com/magazine-design/assets/images/icons-components/icon_header_simple.svg"
    },
    {
      "name": "event-list",
      "html": "<ul doc-container=\"event\" class=\"event-list\"></ul>",
      "label": "Event",
      "description": "Timeline events",
      "allowedParents": [
        "article-container"
      ],
      "iconUrl": "https://livingdocs-assets.s3.amazonaws.com/magazine-design/assets/images/icons-components/icon_unordered_list.svg",
      "directives": {
        "event": {
          "defaultComponents": {
            "paragraph": "event-list-item"
          },
          "defaultContent": [
            {
              "component": "event-list-item"
            }
          ]
        }
      }
    },
    {
      "name": "head-wide",
      "html": "<div class=\"head-wide\"><div class=\"head-wide__container\"><div class=\"head-wide__img\" doc-image=\"image\"><span class=\"head-wide__caption\" doc-editable=\"caption\"></span></div><div class=\"head-wide__text\"><span class=\"head-wide__flag\" doc-editable=\"flag\" doc-optional=\"true\">Flag</span><h1 class=\"head-wide__title\" doc-editable=\"title\">Title</h1><div class=\"head-wide__meta\"><span doc-editable=\"author\">Author</span><span>,</span> <span doc-editable=\"date\">Date</span></div></div></div></div>",
      "label": "Wide header",
      "allowedParents": [
        "article-container"
      ],
      "description": "Header and author line",
      "iconUrl": "https://livingdocs-assets.s3.amazonaws.com/magazine-design/assets/images/icons-components/icon_header_full.svg"
    },
    {
      "name": "subtitle",
      "html": "<h2 class=\"subtitle\"><span doc-editable=\"title\">Subtitle</span></h2>",
      "label": "Subtitle",
      "allowedParents": [
        "article-container"
      ],
      "description": "A subtitle for articles",
      "iconUrl": "https://livingdocs-assets.s3.amazonaws.com/magazine-design/assets/images/icons-components/icon_header_simple.svg"
    },
    {
      "name": "image-full-bleed",
      "html": "<div class=\"image-full-bleed\" doc-image=\"image\"><figcaption class=\"image-full-bleed__caption\" doc-editable=\"caption\" doc-optional=\"true\">Image caption</figcaption></div>",
      "label": "Full Bleed Image",
      "description": "Full width image with scrolling effect",
      "allowedParents": [
        "article-container",
        "author-container",
        "gallery-container",
        "teaser-author-template",
        "author-head",
        "teaser-card-template",
        "teaser-gallery-hero-template",
        "teaser-gallery-template"
      ],
      "iconUrl": "https://livingdocs-assets.s3.amazonaws.com/magazine-design/assets/images/icons-components/icon_image.svg",
      "directives": {
        "image": {
          "imageRatios": [
            "16:9",
            "1:1",
            "4:3",
            "3:4"
          ],
          "allowOriginalRatio": true
        }
      }
    },
    {
      "name": "head",
      "html": "<div class=\"head\"><span class=\"head__flag\" doc-editable=\"flag\" doc-optional=\"true\">Flag</span><h1 class=\"head__title\" doc-editable=\"title\">Title</h1><p class=\"head__lead\" doc-editable=\"text\">Lead</p><div class=\"head__meta\"><span doc-editable=\"author\">Author</span><span>,</span> <span doc-editable=\"date\">Date</span></div></div>",
      "label": "Title and Lead",
      "allowedParents": [
        "article-container"
      ],
      "description": "Header and author line",
      "iconUrl": "https://livingdocs-assets.s3.amazonaws.com/magazine-design/assets/images/icons-components/icon_header_full.svg"
    },
    {
      "name": "image-row",
      "html": "<figure class=\"image-row\"><div class=\"layout layout--narrow image-row__grid\"><div class=\"layout__item image-row__item\"><div class=\"image-row__image\" doc-image=\"image1\"></div></div><div class=\"layout__item image-row__item\"><div class=\"image-row__image\" doc-image=\"image2\"></div></div></div><figcaption class=\"image-row__caption\" doc-editable=\"caption\" doc-optional=\"true\">Image caption</figcaption></figure>",
      "label": "Two Images",
      "description": "Two images in a row",
      "allowedParents": [
        "article-container",
        "author-container",
        "gallery-container",
        "teaser-author-template",
        "author-head",
        "teaser-card-template",
        "teaser-gallery-hero-template",
        "teaser-gallery-template"
      ],
      "iconUrl": "https://livingdocs-assets.s3.amazonaws.com/magazine-design/assets/images/icons-components/icon_image.svg"
    },
    {
      "name": "image",
      "html": "<figure class=\"image\"><img doc-image=\"image\"><figcaption class=\"image__caption\" doc-editable=\"caption\" doc-optional=\"true\">Image caption</figcaption></figure>",
      "label": "Image",
      "properties": [
        "image-width"
      ],
      "description": "Image with caption",
      "allowedParents": [
        "article-container",
        "author-container",
        "gallery-container",
        "teaser-author-template",
        "author-head",
        "teaser-card-template",
        "teaser-gallery-hero-template",
        "teaser-gallery-template"
      ],
      "iconUrl": "https://livingdocs-assets.s3.amazonaws.com/magazine-design/assets/images/icons-components/icon_image.svg",
      "directives": {
        "image": {
          "imageRatios": [
            "16:9",
            "1:1",
            "4:3",
            "3:4"
          ],
          "allowOriginalRatio": true
        }
      }
    },
