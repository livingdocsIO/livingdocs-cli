{
  "name": "livingdocs-cli",
  "description": "Livingdocs Command Line Utility",
  "version": "2.0.0",
  "author": "Livingdocs AG",
  "repository": "livingdocsIO/livingdocs-cli",
  "homepage": "https://github.com/livingdocsIO/livingdocs-cli",
  "bugs": "https://github.com/livingdocsIO/livingdocs-cli/issues",
  "bin": {
    "livingdocs-cli": "./bin/run"
  },
  "scripts": {
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "test": "mocha --forbid-only \"test/**/*.test.js\"",
    "test:coverage": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "posttest": "eslint .",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^5.0.0",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "axios": "^0.23.0",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.3",
    "cookie": "^0.4.1",
    "dedent": "^0.7.0",
    "diff": "^5.1.0",
    "fastify": "^3.25.3",
    "fastify-cors": "^6.0.2",
    "fastify-static": "^4.5.0",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.0",
    "globby": "^10.0.2",
    "html-minifier": "^4.0.0",
    "inquirer": "^7.0.0",
    "javascript-stringify": "^2.0.0",
    "js-beautify": "^1.10.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "nanoid": "^2.1.0",
    "npmlog": "^4.1.2",
    "p-queue": "^6.6.2",
    "pino-pretty": "^3.6.1",
    "prettify-html": "^0.0.2",
    "tough-cookie": "^4.0.0",
    "warning-header-parser": "^2.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^2.0.0",
    "chai": "^4.3.4",
    "eslint": "^8.5.0",
    "eslint-config-oclif": "^4.0.0",
    "mocha": "^10.0.0",
    "nock": "^13.0.0",
    "nyc": "^15.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "livingdocs-cli",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "release": {
    "extends": "@livingdocs/semantic-release-presets/npm-github"
  }
}
