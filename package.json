{
  "name": "livingdocs-cli",
  "description": "Livingdocs Command Line Utility",
  "version": "0.0.0",
  "author": "Livingdocs AG",
  "repository": "livingdocsIO/livingdocs-cli",
  "homepage": "https://github.com/livingdocsIO/livingdocs-cli",
  "bugs": "https://github.com/livingdocsIO/livingdocs-cli/issues",
  "bin": {
    "livingdocs-cli": "./bin/run"
  },
  "scripts": {
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "test": "mocha --forbid-only \"test/**/*.test.js\"",
    "test:coverage": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "posttest": "eslint .",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "@oclif/plugin-not-found": "^1.2.3",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.3",
    "dedent": "^0.7.0",
    "fastify": "^2.10.0",
    "fastify-cors": "^2.1.3",
    "fastify-static": "^2.5.0",
    "fs-extra": "^8.1.0",
    "globby": "^10.0.1",
    "html-minifier": "^4.0.0",
    "inquirer": "^7.0.0",
    "javascript-stringify": "^2.0.0",
    "js-beautify": "^1.10.2",
    "lodash": "^4.17.15",
    "nanoid": "^2.1.0",
    "npmlog": "^4.1.2",
    "p-queue": "^6.1.1",
    "pino-pretty": "^3.2.2",
    "warning-header-parser": "^2.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "chai": "^4.2.0",
    "eslint": "^6.3.0",
    "eslint-config-oclif": "^3.1.0",
    "mocha": "^6.2.0",
    "nock": "^11.3.3",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "livingdocs-cli",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "release": {
    "extends": "@livingdocs/semantic-release-presets/npm-github"
  }
}
